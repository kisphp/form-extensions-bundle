{
    "name": "kisphp/form-extensions-bundle",
    "description": "Kisphp symfony form extension bundle",
    "keywords": ["kisphp", "form", "form extensions bundle", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marius-Bogdan Rizac",
            "email": "mariusbogdan83@gmail.com",
            "homepage": "http://www.kisphp.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1",
        "symfony/var-dumper": "~3.0",
        "friendsofphp/php-cs-fixer": "~1.9",
        "phpstan/phpstan": "^0.6.4",
        "twig/twig": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Kisphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
